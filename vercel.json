{
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "functions": {
    "api": {
      "runtime": "vercel-node@2.0.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/my-api-function"
    },
    {
      "source": "/apps/backend/(.*)",
      "destination": "/apps/backend/server.ts"
    },
    {
      "source": "/apps/frontend/(.*)",
      "destination": "/apps/frontend/index.html"
    }
  ]
}